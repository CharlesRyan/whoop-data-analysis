{
  "name": "Whooper",
  "version": "1.0.0",
  "description": "",
  "author": "Charles Ryan",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "s3": "aws s3 cp backend s3://whooper-backend --recursive",
    "lambda": "aws lambda update-function-code --function-name whooper --zip-file fileb://backend.zip",
    "zip": "zip -r backend.zip backend",
    "py": "python3 backend/lambda_function.py"
  },
  "lint-staged": {
    "*.{js,vue}": "npm run lint",
    "*.{css,vue}": "stylelint"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "axios": "^0.19.2",
    "c3": "^0.7.18",
    "d3": "^5.16.0",
    "nuxt": "^2.0.0",
    "pug": "^3.0.0",
    "pug-plain-loader": "^1.0.0"
  },
  "devDependencies": {
    "@nuxtjs/eslint-config": "^2.0.0",
    "@nuxtjs/eslint-module": "^1.0.0",
    "@nuxtjs/stylelint-module": "^3.1.0",
    "@nuxtjs/vuetify": "^1.11.2",
    "@vue/test-utils": "^1.0.0-beta.27",
    "babel-eslint": "^10.0.1",
    "eslint": "^6.1.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-nuxt": ">=0.4.2",
    "eslint-plugin-prettier": "^3.1.2",
    "husky": "^4.0.0",
    "lint-staged": "^10.0.0",
    "node-sass": "^4.14.1",
    "prettier": "^1.19.1",
    "sass-loader": "^9.0.1",
    "stylelint": "^10.1.0"
  }
}
